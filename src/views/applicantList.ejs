<div><div class="container">
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Contact</th>
          <th scope="col">Resume</th>
        </tr>
      </thead>
      <tbody>
        <% applicant.forEach((user,index)=>{ %>

          <tr>
            <th scope="row"><%= index+1 %></th>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.contact %></td>
            <td>
              <a class="btn btn-sm btn-primary" href="/resume/<%=user.resumePath %> " target="_blank">view</a>
            </td>
          </tr>
          <% }) %>
        
      </tbody>
    </table>
  </div>
  </div>